<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:orientation="vertical"
    tools:context="bni.emplus.DaftarPenerima">
    <ListView
        android:layout_height="fill_parent"
        android:layout_width="fill_parent"
        android:id="@+id/lvDaftarPenerima"
        android:paddingLeft="@dimen/activity_horizontal_margin"
        android:layout_above="@+id/tool_bar_bottom"/>
    <include
        android:id="@+id/tool_bar_bottom"
        layout="@layout/toolbar_bottom_tujuh"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"></include>
</RelativeLayout>

